<!-- action = what route to use -->
<!-- method = what verb to use -->
<form action="/dinosaurs" method="POST">
<!-- name = key that this data will enter our app on -->
<!-- id matches up the label and the input -->
    <label for="dinoName">Name</label>
    <input type="text" name="name" id="dinoName">
    <label for="dinoType">Type</label>
    <input type="text" name="name" id="dinoType">
    <input type="submit">
</form>